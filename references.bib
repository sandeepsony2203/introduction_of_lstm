@Article{Beltramelli-2017-p-,
  author        = {Tony Beltramelli},
  title         = {pix2code: Generating Code from a Graphical User Interface Screenshot},
  journal       = {CoRR},
  year          = {2017},
  volume        = {abs/1705.07962},
  archiveprefix = {arXiv},
  bibsource     = {dblp computer science bibliography, http://dblp.org},
  biburl        = {http://dblp.org/rec/bib/journals/corr/Beltramelli17},
  eprint        = {1705.07962},
  file          = {Beltramelli-2017-p-.pdf:FullText\\Beltramelli-2017-p-.pdf:PDF},
  groups        = {代码生成},
  review        = {pix2code，采用CNN获得图像的特征，利用RNN获得代码上下文关系，来预测新图片对应的代码。另外，这里用的代码不是目标代码，而是中间代码，DSL，也叫领域专用语言，其文本空间非常小，很利于RNN预测。

目前常用的编程语言有很多，比如JAVA、C++、Python、PHP等，对于机器码农来说，也存在着：“PHP是不是最好的语言”这种问题，就是说需要作出应该用什么语言产生代码的决策。一般不同的深度学习自动编码系统都会参考“领域特定语言（Domain Specific Language）”自己定义一种编程语言，而不是直接采用人类程序员常用的某种编程语言，这是因为对于机器产生代码来说，目前常用的编程语言过于复杂，里面包含了循环、分支判断等复杂控制逻辑，而根据“领域特定语言”定义的语言一般都比较简单，不包含这些控制逻辑，只包含一些基本原语，比如数值加一、数值减一、移动指针位置、读取某存储器内数据、将数据写入存储器某位置等基本操作原语。所有代码片段（包括训练数据对应的程序以及将来要生成的代码）都采用自定义的领域特定语言，这样会大大简化机器码农的学习难度。},
  timestamp     = {Wed, 07 Jun 2017 14:42:16 +0200},
  url           = {http://arxiv.org/abs/1705.07962},
}

